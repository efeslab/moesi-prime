    }
  }

  action(u_writeDataToCacheClean, "uc", desc="Write data to cache") {
    peek(responseToL1Cache_in, ResponseMsg) {
      assert(is_valid(cache_entry));
      assert(in_msg.Dirty);
      cache_entry.DataBlk := in_msg.DataBlk;
      cache_entry.Dirty := false;
    }
  }

  action(kk_deallocateL1CacheBlock, "\k", desc="Deallocate cache block.  Sets the cache to invalid, allowing a replacement in parallel with a fetch.") {
    if (L1Dcache.isTagPresent(address)) {
      L1Dcache.deallocate(address);
    } else {
      L1Icache.deallocate(address);
    }
    unset_cache_entry();
  }

  action(ii_allocateL1DCacheBlock, "\i", desc="Set L1 D-cache tag equal to tag of block B.") {
    if ((is_invalid(cache_entry))) {
      set_cache_entry(L1Dcache.allocate(address, new Entry));
    }
  }

  action(jj_allocateL1ICacheBlock, "\j", desc="Set L1 I-cache tag equal to tag of block B.") {
    if ((is_invalid(cache_entry))) {
      set_cache_entry(L1Icache.allocate(address, new Entry));
    }
  }

  action(forward_eviction_to_cpu, "\cc", desc="sends eviction information to the processor") {
    if (send_evictions) {
      DPRINTF(RubySlicc, "Sending invalidation for %#x to the CPU\n", address);
      sequencer.evictionCallback(address);
    }
  }

  action(uu_profileInstMiss, "\uim", desc="Profile the demand miss") {
    L1Icache.profileDemandMiss();
  }

  action(uu_profileInstHit, "\uih", desc="Profile the demand hit") {
    L1Icache.profileDemandHit();
  }

  action(uu_profileDataMiss, "\udm", desc="Profile the demand miss") {
    L1Dcache.profileDemandMiss();
  }

  action(uu_profileDataHit, "\udh", desc="Profile the demand hit") {
    L1Dcache.profileDemandHit();
  }

  action(st_stallAndWaitMandatoryQueue, "st", desc="Stall and wait on the address") {
    stall_and_wait(mandatoryQueue_in, address);
  }

  // action(st_stallAndWaitRequestQueue, "str", desc="Stall and wait on the address") {
  //   stall_and_wait(requestNetwork_in, address);
  // }

  action(wa_wakeUpAllDependents, "wa", desc="Wake up any requests waiting for this address") {
    wakeUpAllBuffers(address);
  }

  //*****************************************************
  // TRANSITIONS
  //*****************************************************

  // Transitions for Load/Store/L2_Replacement from transient states
  transition({IM, SM, OM, IS, ISF, OI, SI, MI, II}, {Store, L1_Replacement}) {
    st_stallAndWaitMandatoryQueue;
  }

  // transition({MM_W, M_W}, L1_Replacement) {
  //   st_stallAndWaitMandatoryQueue;
  // }

  // transition({MM_W, M_W}, {Fwd_GETS, Fwd_GETO, Fwd_DMA, Fwd_GETX, Own_GETX, Inv}) {
  //   st_stallAndWaitRequestQueue;
  // }

  transition({IM, IS, ISF, OI, MI, SI, II}, {Load, Ifetch}) {
    st_stallAndWaitMandatoryQueue;
  }

  // Transitions from Idle
  transition(I, Load, IS) {
    ii_allocateL1DCacheBlock;
    i_allocateTBE;
    a_issueGETS;
    uu_profileDataMiss;
    k_popMandatoryQueue;
  }

  transition(I, Ifetch, ISF) {
    jj_allocateL1ICacheBlock;
    i_allocateTBE;
    a_issueGETS;
    uu_profileInstMiss;
    k_popMandatoryQueue;
  }

  transition(I, Store, IM) {
    ii_allocateL1DCacheBlock;
    i_allocateTBE;
    b_issueGETX;
    uu_profileDataMiss;
    k_popMandatoryQueue;
  }

  transition(I, L1_Replacement) {
    kk_deallocateL1CacheBlock;
  }

  transition(I, Inv) {
    f_sendAck;
    l_popForwardQueue;
  }

  transition({S, SM, O, OM, M, MM}, Load) {
    h_load_hit;
    uu_profileDataHit;
    k_popMandatoryQueue;
  }

  transition({S, SM, O, OM, M, MM}, Ifetch) {
    h_ifetch_hit;
    uu_profileInstHit;
    k_popMandatoryQueue;
  }

  // Transitions from Shared
  transition(S, Store, SM) {
    i_allocateTBE;
    b_issueGETX;
    uu_profileDataMiss;
    k_popMandatoryQueue;
  }

  transition(S, L1_Replacement, SI) {
    i_allocateTBE;
    mdvt_markDataValidTBE;
    dd_issuePUTS;
    forward_eviction_to_cpu;
    kk_deallocateL1CacheBlock;
  }

  transition(S, Inv, I) {
    f_sendAck;
    forward_eviction_to_cpu;
    l_popForwardQueue;
  }

  transition(S, Fwd_GETS) {
    e_sendData;
    l_popForwardQueue;
  }

  transition(S, Fwd_DMA) {
    e_sendDataDMA;
    ub_dmaUnblockL2Cache;
    l_popForwardQueue;
  }

  // Transitions from Owned
  transition(O, Store, OM) {
    i_allocateTBE;
    b_issueGETX;
    uu_profileDataMiss;
    k_popMandatoryQueue;
  }

  transition(O, L1_Replacement, OI) {
    i_allocateTBE;
    mdvt_markDataValidTBE;
    dd_issuePUTO;
    forward_eviction_to_cpu;
    kk_deallocateL1CacheBlock;
  }

  transition(O, Fwd_GETX, I) {
    ee_sendDataExclusive;
    forward_eviction_to_cpu;
    l_popForwardQueue;
  }

  transition(O, Fwd_GETS) {
    e_sendData;
    l_popForwardQueue;
  }

  transition(O, Fwd_GETO_Local) {
    e_sendDataAndDirty;
    l_popForwardQueue;
  }

  transition(O, Fwd_GETO, S) {
    e_sendDataAndClean;
    l_popForwardQueue;
  }

  transition(O, Fwd_DMA) {
    e_sendDataDMA;
    ub_dmaUnblockL2Cache;
    l_popForwardQueue;
  }

  // Transitions from M
  transition(M, Store, MM) {
    hh_store_hit;
    uu_profileDataHit;
    k_popMandatoryQueue;
  }

  // transition(M_W, Store, MM_W) {
  //   hh_store_hit;
  //   uu_profileDataHit;
  //   k_popMandatoryQueue;
  // }

  transition({MM}, Store) {
    hh_store_hit;
    uu_profileDataHit;
    k_popMandatoryQueue;
  }

  transition({M, MM}, L1_Replacement, MI) {
    i_allocateTBE;
    mdvt_markDataValidTBE;
    d_issuePUTX;
    forward_eviction_to_cpu;
    kk_deallocateL1CacheBlock;
  }

  transition({M, MM}, Fwd_GETX, I) {
    // e_sendData;
    ee_sendDataExclusive;
    forward_eviction_to_cpu;
    l_popForwardQueue;
  }

  transition(MM, {Fwd_GETO_Local, Fwd_GETO, Fwd_GETS}, I) {
    ee_sendDataExclusive;
    forward_eviction_to_cpu;
    l_popForwardQueue;
  }

  transition(M, Fwd_GETS, O) {
    e_sendData;
    l_popForwardQueue;
  }

  transition(M, Fwd_GETO_Local, O) {
    e_sendDataAndDirty;
    l_popForwardQueue;
  }

  transition(M, Fwd_GETO, S) {
    e_sendDataAndClean;
    l_popForwardQueue;
  }

  transition({M, MM}, Fwd_DMA) {
    e_sendDataDMA;
    ub_dmaUnblockL2Cache;
    l_popForwardQueue;
  }

  // Transitions from IM

  transition(IM, Inv) {
    f_sendAck;
    l_popForwardQueue;
  }

  transition(IM, Ack) {
    m_decrementNumberOfMessages;
    o_checkForCompletion;
    n_popResponseQueue;
  }

  transition(IM, {Exclusive_Data_Dirty, Exclusive_Data_Clean, Exclusive_Data_Clean_C2C, Data}, OM) {
    u_writeDataToCache;
    m_decrementNumberOfMessages;
    o_checkForCompletion;
    n_popResponseQueue;
  }

  // Transitions from SM
  transition(SM, Inv, IM) {
    f_sendAck;
    forward_eviction_to_cpu;
    mdivt_markDataInvalidTBE;
    l_popForwardQueue;
  }

  transition(SM, Ack) {
    m_decrementNumberOfMessages;
    o_checkForCompletion;
    n_popResponseQueue;
  }

  transition(SM, {Data, Exclusive_Data_Dirty, Exclusive_Data_Clean, Exclusive_Data_Clean_C2C}, OM) {
    // v_writeDataToCacheVerify;
    u_writeDataToCache;
    m_decrementNumberOfMessages;
    o_checkForCompletion;
    n_popResponseQueue;
  }

  transition(SM, Fwd_GETS) {
    e_sendData;
    l_popForwardQueue;
  }

  transition(SM, Fwd_DMA) {
    e_sendDataDMA;
    ub_dmaUnblockL2Cache;
    l_popForwardQueue;
  }

  // Transitions from OM
  transition(OM, Own_GETX) {
    mm_decrementNumberOfMessages;
    o_checkForCompletion;
    l_popForwardQueue;
  }


  // transition(OM, Fwd_GETX, OMF) {
  transition(OM, Fwd_GETX, IM) {
    ee_sendDataExclusive;
    mdivt_markDataInvalidTBE;
    forward_eviction_to_cpu;
    l_popForwardQueue;
  }

  transition(OM, Fwd_GETO, SM) {
    e_sendDataAndClean;
    l_popForwardQueue;
  }

  transition(OM, Fwd_GETS) {
    e_sendData;
    l_popForwardQueue;
  }

  transition(OM, Fwd_GETO_Local) {
    e_sendDataAndDirty;
    l_popForwardQueue;
  }

  transition(OM, Fwd_DMA) {
    e_sendDataDMA;
    ub_dmaUnblockL2Cache;
    l_popForwardQueue;
  }

  //transition({OM, OMF}, Ack) {
  transition(OM, Ack) {
    m_decrementNumberOfMessages;
    o_checkForCompletion;
    n_popResponseQueue;
  }

  transition(OM, All_acks, MM) {
    gg_sendUnblockExclusive;
    xx_store_hit;
    // o_scheduleUseTimeout;
    s_deallocateTBE;
    j_popTriggerQueue;
    wa_wakeUpAllDependents;
  }

  // transition(MM_W, Use_Timeout, MM) {
  //   jj_unsetUseTimer;
  //   wa_wakeUpAllDependents;
  // }

  // transition(M_W, Use_Timeout, M) {
  //   jj_unsetUseTimer;
  //   wa_wakeUpAllDependents;
  // }

  // Transitions from IS

  transition({IS, ISF}, Inv) {
    f_sendAck;
    l_popForwardQueue;
  }

  transition({IS, ISF}, Data, S) {
    u_writeDataToCache;
    m_decrementNumberOfMessages;
    hx_load_hit;
    g_sendUnblock;
    s_deallocateTBE;
    n_popResponseQueue;
    wa_wakeUpAllDependents;
  }

  transition({IS, ISF}, Data_Owner_Dirty, O) {
    u_writeDataToCache;
    m_decrementNumberOfMessages;
    hx_load_hit;
    g_sendUnblockOwner;
    s_deallocateTBE;
    n_popResponseQueue;
    wa_wakeUpAllDependents;
  }

  transition({IS, ISF}, Data_Owner_Force_Clean, S) {
    u_writeDataToCacheClean;
    m_decrementNumberOfMessages;
    hx_load_hit;
    g_sendUnblockOwner;
    s_deallocateTBE;
    n_popResponseQueue;
    wa_wakeUpAllDependents;
  }

  transition({IS, ISF}, Data_Owner_Clean, S) {
    u_writeDataToCache;
    m_decrementNumberOfMessages;
    hx_load_hit;
    g_sendUnblockOwner;
    s_deallocateTBE;
    n_popResponseQueue;
    wa_wakeUpAllDependents;
  }

  // kevlough: for inst fetch, don't do excl upgrade
  transition(ISF, Exclusive_Data_Clean, S) {
    u_writeDataToCache;
    m_decrementNumberOfMessages;
    hx_load_hit;
    g_sendUnblock;
    s_deallocateTBE;
    n_popResponseQueue;
    wa_wakeUpAllDependents;
  }

  transition(ISF, {Exclusive_Data_Dirty, Exclusive_Data_Clean_C2C}, M) {
    u_writeDataToCache;
    m_decrementNumberOfMessages;
    gg_sendUnblockExclusive;
    hx_load_hit;
    // o_scheduleUseTimeout;
    s_deallocateTBE;
    n_popResponseQueue;
    wa_wakeUpAllDependents;
  }

  // kevlough: this is really the E state, but encoded as M in
  // this gem5 version. On writeback, clean/dirty distinguishes
  transition(IS, Exclusive_Data_Clean, M) {
    u_writeDataToCache;
    m_decrementNumberOfMessages;
    gg_sendUnblockExclusive;
    hx_load_hit;
    s_deallocateTBE;
    n_popResponseQueue;
    wa_wakeUpAllDependents;
  }

  transition(IS, {Exclusive_Data_Clean_C2C, Exclusive_Data_Dirty}, M) {
    u_writeDataToCache;
    m_decrementNumberOfMessages;
    gg_sendUnblockExclusive;
    hx_load_hit;
    // o_scheduleUseTimeout;
    s_deallocateTBE;
    n_popResponseQueue;
    wa_wakeUpAllDependents;
  }

  // Transitions from OI/MI

  transition(MI, Fwd_GETS, OI) {
    q_sendDataFromTBEToCache;
    l_popForwardQueue;
  }

  transition({MI, OI}, Fwd_GETO_Local, OI) {
    q_sendDataFromTBEToCacheAndDirty;
    l_popForwardQueue;
  }

  transition({MI, OI}, Fwd_GETO, SI) {
    q_sendDataFromTBEToCacheAndClean;
    l_popForwardQueue;
  }

  transition(MI, Fwd_DMA) {
    q_sendDataFromTBEToDMA;
    ub_dmaUnblockL2Cache;
    l_popForwardQueue;
  }

  transition(MI, Fwd_GETX, II) {
    q_sendExclusiveDataFromTBEToCache;
    mdivt_markDataInvalidTBE;
    l_popForwardQueue;
  }

  transition({SI, OI}, Fwd_GETS) {
    q_sendDataFromTBEToCache;
    l_popForwardQueue;
  }

  transition({SI, OI}, Fwd_DMA) {
    q_sendDataFromTBEToDMA;
    ub_dmaUnblockL2Cache;
    l_popForwardQueue;
  }

  transition(OI, Fwd_GETX, II) {
    q_sendExclusiveDataFromTBEToCache;
    mdivt_markDataInvalidTBE;
    l_popForwardQueue;
  }

  transition(SI, Writeback_Ack_Data, I) {
    qq_sendWBDataFromTBEToL2NotOwner;  // always send data
    s_deallocateTBE;
    n_popResponseQueue;
    wa_wakeUpAllDependents;
  }

  transition({OI, MI}, Writeback_Ack_Data, I) {
    qq_sendWBDataFromTBEToL2;  // always send data
    s_deallocateTBE;
    n_popResponseQueue;
    wa_wakeUpAllDependents;
  }

  transition({SI, OI, MI}, Writeback_Ack, I) {
    g_sendUnblockInvalid;
    s_deallocateTBE;
    n_popResponseQueue;
    wa_wakeUpAllDependents;
  }

  transition({MI, OI}, Writeback_Nack, OI) {
    // FIXME: This might cause deadlock by re-using the writeback
    // channel, we should handle this case differently.
    dd_issuePUTO;
    n_popResponseQueue;
  }

  // Transitions from II
  transition(II, {Writeback_Ack, Writeback_Ack_Data}, I) {
    g_sendUnblockInvalid;
    s_deallocateTBE;
    n_popResponseQueue;
    wa_wakeUpAllDependents;
  }

  // transition({II, SI}, Writeback_Nack, I) {
  transition(II, Writeback_Nack, I) {
    s_deallocateTBE;
    n_popResponseQueue;
    wa_wakeUpAllDependents;
  }

  transition(SI, Writeback_Nack) {
    dd_issuePUTS;
    n_popResponseQueue;
  }

  transition(II, Inv) {
    f_sendAck;
    l_popForwardQueue;
  }

  transition(SI, Inv, II) {
    f_sendAck;
    mdivt_markDataInvalidTBE;
    l_popForwardQueue;
  }
}
